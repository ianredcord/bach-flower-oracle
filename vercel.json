{
  "rewrites": [
    {
      "source": "/result/:id",
      "destination": "/api/result/:id"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
